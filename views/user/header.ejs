<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>

    <style>
        /* Your existing styles */
       
        .right {
            display: flex;
            align-items: center;
            margin-left: auto;
        }
        
        .right form {
            margin-right: 10px;
            /* Adjust the margin as needed */
        }
        
        .right .nav-icons {
            display: flex;
            align-items: center;
        }
        
        .right .nav-item {
            margin-right: 10px;
            /* Adjust the margin as needed */
        }
        
        .form-control-header {
            width: 45px;
            height: 30px;
            border-radius: 20px;
            border: none;
            background: rgb(235, 235, 235);
        }
        
        .navbar {
            box-shadow: 0 5px 8px rgba(0, 0, 0, 0.2);
        }
        
        @media screen and (min-width: 992px) {
            .navbar {
                padding-top: 0px;
                padding-bottom: 0px;
            }
            .navbar .nav-item {
                padding: .5rem .5rem;
                margin: 0 .25rem;
            }
        }
        /* Adjust the font size for specific icons */
        
        .fa-shopping-cart,
        .fa-user-circle,
        .fa-heart,.fa-wallet,
        .fa-sign-out {
            font-size: 28px;
            color: rgb(66, 95, 117);
        }
        
        .nav-link-header,
        .nameheader {
            font-size: 20px;
            color: black;
            font-weight: 600;
            text-decoration: none;
        }
        .cart-quantity {
            /* position: absolute; */
            top: -8px;
            right: -8px;
            background-color: red;
            color: white;
            font-size: 12px;
            padding: 2px 5px;
            border-radius: 50%;
}
    </style>

    <!-- Your existing stylesheets and scripts -->

</head>

<body>

    <!-- Your existing HTML structure -->

    <nav class="navbar navbar-expand-lg navbar-dark bg-light">
        <a class="navbar-brand" href="/home" style="color: black;font-size: 50px;margin-left: 2%;font-weight: 500;">TJ Watches</a>

        <a class="nav-link-header text-dark" href="/user/Mens" style="margin-left: 2%;">
            <!-- <i class="fa fa-male"></i> -->
            MEN
        </a>

        <a class="nav-link-header text-dark" href="/user/Women" style="margin-left: 2%;">
            <!-- <i class="fas fa-female" ></i> -->
            FEMALE
        </a>
        <!-- Your existing content -->

        <div class="right" style="margin-right: 3%;">
            <form class="form-inline my-2 my-lg-0">
                <!-- Your existing form content -->
            </form>
            <div style="float: left;">
                <a href="#" class="nav-item nameheader">Hi <%= name %></i>
                <a href="/user/wishlist/<%= userid %>" class="nav-item"><i class="fas fa-heart"></i></a>
                <a href="/user/cart/<%= userid %>" class="nav-item"><i class="fas fa-shopping-cart"></i></a>
                <a href="/user/wallet/<%= userid %>" class="nav-item "><i class="fa fa-wallet"></i></a>
                <a href="/profile" class="nav-item"><i class="fa fa-user-circle"></i></a>
                <a href="/userLogout" class="nav-item"><i class="fa fa-sign-out" aria-hidden="true"></i>
                </a>

            </div>
        </div>
    </nav>
<script>
    // Assuming you have a variable named 'cartQuantity' that holds the total quantity of products in the cart
// Replace this with the actual quantity from your server or client-side storage
// const cartQuantity=document.getElementById('count')
const cartQuantity= parseInt(document.getElementById('count').textContent, 10);

// Get the cart icon element
const cartIcon = document.querySelector('.fa-shopping-cart');

// Create a span element for the superscript
const cartQuantitySpan = document.createElement('span');
cartQuantitySpan.className = 'cart-quantity';
cartQuantitySpan.textContent = cartQuantity;

// Append the superscript span to the cart icon
cartIcon.appendChild(cartQuantitySpan);
</script>
    <!-- Your existing content -->

</body>

</html>